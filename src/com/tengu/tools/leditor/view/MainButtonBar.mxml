<?xml version="1.0" encoding="utf-8"?>
<s:ButtonBar xmlns:fx="http://ns.adobe.com/mxml/2009" 
			 xmlns:s="library://ns.adobe.com/flex/spark" 
			 xmlns:mx="library://ns.adobe.com/flex/mx"
			 currentState="defaultState" 
			 styleName="iconBar"
			 requireSelection="true">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
		]]>
	</fx:Script>
	<s:states>
		<s:State name="infiniteBitmap"/>
		<s:State name="imageTiles"/>
		<s:State name="objects"/>
		<s:State name="defaultState"/>
	</s:states>
	
	<s:dataProvider>
		<s:ArrayCollection>
			<fx:Object icon="@Embed('/../assets/icons/cursor.png')"/>
			<fx:Object icon="@Embed('/../assets/icons/move.png')" includeIn="infiniteBitmap,defaultState"/>
		</s:ArrayCollection>
	</s:dataProvider>

</s:ButtonBar>
